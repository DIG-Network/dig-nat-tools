{
  "name": "@dignetwork/dig-nat-tools",
  "version": "1.1.1",
  "description": "Decentralized P2P file transfer with NAT traversal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "example": "ts-node src/examples/simple-example.ts",
    "example:host": "ts-node src/examples/host-example.ts",
    "example:client": "ts-node src/examples/client-example.ts",
    "example:multi-peer": "ts-node src/examples/multi-peer-client-example.ts",
    "example:ip-discovery": "ts-node src/examples/ip-discovery-example.ts",
    "example:simple-host": "ts-node src/examples/simple-host-example.ts",
    "example:simple-client": "ts-node src/examples/simple-client-example.ts",
    "example:directory-watcher": "ts-node src/examples/directory-watcher-example.ts",
    "example:shard-host": "ts-node src/examples/shard-host-example.ts",
    "prepare-release": "bash ./scripts/release.sh"
  },
  "keywords": [
    "p2p",
    "nat",
    "traversal",
    "file",
    "transfer",
    "webrtc",
    "udp",
    "tcp",
    "gun"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DIG-Network/dig-nat-tools"
  },
  "dependencies": {
    "@noble/ed25519": "^2.2.3",
    "@noble/hashes": "^1.7.1",
    "@noble/secp256k1": "^2.2.3",
    "@types/k-bucket": "^5.0.4",
    "chokidar": "^3.6.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.3.0",
    "gun": "^0.2020.1239",
    "ip": "^1.1.9",
    "k-bucket": "^5.1.0",
    "lodash.debounce": "^4.0.8",
    "lru-cache": "^11.0.2",
    "nat-upnp": "^1.1.1",
    "node-datachannel": "^0.5.2",
    "node-fetch": "^3.3.2",
    "strong-soap": "^4.1.10",
    "stun": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.4",
    "@types/ip": "^1.1.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.8.0",
    "@types/node-persist": "^3.1.8",
    "@types/uuid": "^9.0.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
