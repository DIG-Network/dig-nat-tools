{
  "name": "dig-nat-tools",
  "version": "1.0.0",
  "description": "Decentralized P2P file transfer with NAT traversal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "example": "ts-node src/examples/simple-example.ts",
    "example:host": "ts-node src/examples/host-example.ts",
    "example:client": "ts-node src/examples/client-example.ts",
    "example:multi-peer": "ts-node src/examples/multi-peer-client-example.ts",
    "example:ip-discovery": "ts-node src/examples/ip-discovery-example.ts"
  },
  "keywords": [
    "p2p",
    "nat",
    "traversal",
    "file",
    "transfer",
    "webrtc",
    "udp",
    "tcp",
    "gun"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dig-nat-tools.git"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "gun": "^0.2020.1239",
    "ip": "^1.1.9",
    "node-datachannel": "^0.5.2",
    "uuid": "^9.0.0",
    "stun": "^2.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.2",
    "@types/ip": "^1.1.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
